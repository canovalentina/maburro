{
    "manifest_version": 2,
    "name": "Maburro",
    "version": "1.0",
    "description": "Adds a quote to any mention of Nicolas Maduro.",
    "content_scripts": 
    [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}